<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="classpath*:/META-INF/spring/env-context.xml" />
	<import resource="classpath*:/META-INF/spring/mail-context.xml" />

	<bean id="djatokaURLBuilder" class="com.bhle.access.util.DjatokaURLBuilder"
		p:baseUrl="${djatoka.baseUrl}" />

	<bean class="com.bhle.access.download.Resolution">
		<property name="map">
			<map>
				<entry>
					<key>
						<value>high</value>
					</key>
					<value>${djatoka.level.high}</value>
				</entry>
				<entry>
					<key>
						<value>medium</value>
					</key>
					<value>${djatoka.level.medium}</value>
				</entry>
				<entry>
					<key>
						<value>low</value>
					</key>
					<value>${djatoka.level.low}</value>
				</entry>
			</map>
		</property>
	</bean>

</beans>
