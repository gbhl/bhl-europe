/**
 * Implements hook_theme().
 */
function views_jqfx_cloudcarousel_theme($existing, $type, $theme, $path) {
  return array(
    'views_jqfx_cloudcarousel' => array(
      'variables' => array('view' => NULL, 'options' => array(), 'rows' => array(), 'title' => ''),
      'template' => 'views-jqfx-cloudcarousel',
      'file' => 'views_jqfx_cloudcarousel.theme.inc',
      'path' => $path . '/theme',
    ),
  );
}
