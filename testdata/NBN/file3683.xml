   <dc:title>Revision of the genus Paratropus Gerstaecker (Coleoptera: Histeridae). II. Supplement</dc:title>
   <dc:title>Zoologische Mededelingen</dc:title>
   <dc:creator>Kanaar P.</dc:creator>
   <dc:subject>Insecta</dc:subject>
   <dc:subject>Coleoptera</dc:subject>
   <dc:subject>Histeridae</dc:subject>
   <dc:subject>Paratropus</dc:subject>
