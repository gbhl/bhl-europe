   <dc:subject>Encarsia</dc:subject>
   <dc:subject>Aleyrodidae</dc:subject>
   <dc:subject>Aphelinidae</dc:subject>
   <dc:subject>Diaspididae</dc:subject>
   <dc:subject>parasitoids</dc:subject>
   <dc:subject>biological control</dc:subject>
